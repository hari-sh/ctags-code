{
  "name": "ctags-code",
  "displayName": "ctags-code",
  "icon": "icon.png",
  "description": "",
  "publisher": "harikalyan",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages",
    "Language Packs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hari-sh/ctags-code"
  },
  "activationEvents": [],
  "main": "./src/extension.js",
  "scripts": {
    "compile": "webpack --mode development",
    "build": "webpack --mode production",
    "package": "vsce package",
    "vscode:prepublish": "webpack"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.storeTags",
        "title": "CTags: Store Tags"
      },
      {
        "command": "extension.jumpTag",
        "title": "CTags: Jump To Tag"
      },
      {
        "command": "extension.searchTags",
        "title": "CTags: Search Tags"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.jumpTag",
          "when": "editorTextFocus",
          "group": "navigation"
        }
      ]
    }
  },
    "dependencies": {
    "level": "^10.0.0"
  },
  "devDependencies": {
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  }
}
